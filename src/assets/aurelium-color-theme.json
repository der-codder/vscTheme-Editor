{
  "$schema": "vscode://schemas/color-theme",
  "name": "Aurelium",
  "type": "dark",
  "colors": {
    "button.background": "#097bed",
    "button.hoverBackground": "#4a90e2",

    "dropdown.background": "#6e6e6e",
    "dropdown.border": "#5a5a5a",

    "input.background": "#464646",
    "input.border": "#787878",

    "badge.background": "#252525",
    "badge.foreground": "#7ed321",

    "list.activeSelectionBackground": "#464646",
    "list.activeSelectionForeground": "#d7dae0",
    "list.focusBackground": "#6e6e6e",
    "list.hoverBackground": "#3c3c3c",
    "list.highlightForeground": "#C5C5C5",
    "list.inactiveSelectionBackground": "#464646",
    "list.inactiveSelectionForeground": "#d7dae0",

    "activityBar.background": "#4c4c4c",
    "activityBarBadge.background": "#f47023",

    "sideBar.background": "#333",
    "sideBar.border": "#5a5a5a",
    "sideBarSectionHeader.background": "#3c3c3c",

    "editorSuggestWidget.highlightForeground": "#7ed321",
    "editorGroup.border": "#5a5a5a",
    "editorGroupHeader.tabsBackground": "#4c4c4c",
    "tab.activeBackground": "#3c3c3c",
    "tab.border": "#464646",
    "tab.activeBorder": "#f47023",
    "tab.inactiveBackground": "#333",

    "panelTitle.activeBorder": "#f47023",

    "statusBar.background": "#323232",
    "statusBar.border": "#505050",
    "statusBar.foreground": "#9da5b4",
    "statusBarItem.hoverBackground": "#2c313a",
    "statusBar.noFolderBackground": "#323232",
    "statusBar.debuggingBackground": "#7e0097",
    "statusBar.debuggingBorder": "#66017a",
    "statusBar.debuggingForeground": "#ffffff",

    "editor.background": "#1E1E1E",
    "editor.foreground": "#D4D4D4",
    "editor.lineHighlightBackground": "#2b2b2b",
    "editor.inactiveSelectionBackground": "#3A3D41",
    "editor.selectionHighlightBackground": "#ADD6FF26",
    "editorIndentGuide.background": "#404040",
    "editorLineNumber.activeForeground": "#c2c2c2",
    "list.dropBackground": "#383B3D",
    "sideBarTitle.foreground": "#BBBBBB",
    
    "terminal.ansiBlack": "#737F9A",
    "terminal.ansiRed": "#FF395B",
    "terminal.ansiGreen": "#4CAF50",
    "terminal.ansiBlue": "#2A99F4",
    "terminal.ansiCyan": "#00ACC1",
    "terminal.ansiMagenta": "#C580D1",
    "terminal.ansiYellow": "#FBB814",
    "terminal.ansiWhite": "#E1E1E1",
    "terminal.ansiBrightBlack": "#78909C",
    "terminal.ansiBrightRed": "#FF6C85",
    "terminal.ansiBrightGreen": "#66BB6A",
    "terminal.ansiBrightBlue": "#42A5F5",
    "terminal.ansiBrightCyan": "#00BCD4",
    "terminal.ansiBrightMagenta": "#CE93D8",
    "terminal.ansiBrightYellow": "#FBC02D",
    "terminal.ansiBrightWhite": "#EDEDED"
  },
  "tokenColors": [
    {
      "name": "Object literals",
      "scope": [
        "meta.objectliteral",
        "punctuation.definition.dict",
        "punctuation.definition.dictionary.begin.json",
        "punctuation.definition.dictionary.end.json"
      ],
      "settings": {
        "foreground": "#FFFF00"
      }
    },
    {
      "name": "Array literals",
      "scope": [
        "meta.array.literal",
        "punctuation.definition.list",
        "punctuation.definition.array.begin.json",
        "punctuation.definition.array.end.json"
      ],
      "settings": {
        "foreground": "#18FFFF"
      }
    },
    {
      "name": "Reset 'Array & object literals' side effects",
      "scope": [
        "meta.brace.round",
        "meta.function.expression",
        "meta.method.declaration",
        "punctuation.accessor",
        "punctuation.separator",
        "punctuation.definition.parameters"
      ],
      "settings": {
        "foreground": "#D4D4D4"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#78909C"
      }
    },
    {
      "name": "Documenting string",
      "scope": "string.quoted.docstring.multi.python",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#A1887F"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#F57C00"
      }
    },
    {
      "name": "Template Expression",
      "scope": [
        "punctuation.definition.template-expression",
        "punctuation.definition.interpolation"
      ],
      "settings": {
        "foreground": "#CE93D8"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#9FA8DA"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "foreground": "#9FA8DA"
      }
    },
    {
      "name": "User-defined constant",
      "scope": [
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#9FA8DA"
      }
    },
    {
      "name": "Variable and parameter name",
      "scope": [
        "variable",
        "variable.other.property",
        "variable.other.readwrite",
        "variable.other.object",
        "meta.definition.variable.name",
        "support.variable",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#4DD0E1"
      }
    },
    {
      "name": "Object keys",
      "scope": [
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#CE93D8"
      }
    },
    {
      "name": "this/self/cls",
      "scope": [
        "variable.language.this",
        "keyword.other.this.cs",
        "variable.language.special.self.python",
        "variable.parameter.function.language.special.self.python",
        "variable.language.special.cls.python",
        "variable.parameter.function.language.special.cls.python"
      ],
      "settings": {
        "foreground": "#F50057"
      }
    },
    {
      "name": "Parameter Function self/cls",
      "scope": [
        "variable.parameter.function.language.special.self.python",
        "variable.parameter.function.language.special.cls.python"
      ],
      "settings": {
        "foreground": "#F50057CE"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#03A9F4"
      }
    },
    {
      "name": "Separator Key-value",
      "scope": [
        "punctuation.separator.key-value",
        "punctuation.separator.dictionary.key-value.json"
      ],
      "settings": {
        "foreground": "#03A9F4"
      }
    },
    {
      "name": "Operator Assignment and Arithmetic",
      "scope": [
        "keyword.operator.assignment",
        "keyword.operator.arithmetic"
      ],
      "settings": {
        "foreground": "#D4D4D4"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.cast",
        "keyword.operator.sizeof"
      ],
      "settings": {
        "foreground": "#03A9F4"
      }
    },
    {
      "name": "Operators Logical",
      "scope": [
        "keyword.operator.logical",
        "keyword.operator.relational",
        "keyword.operator.comparison."
      ],
      "settings": {
        "foreground": "#F06292"
      }
    },
    {
      "name": "Operator Arrow",
      "scope": [
        "keyword.operator.arrow.cs",
        "storage.type.function.arrow"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#B388FF"
      }
    },
    {
      "name": "Punctuation Terminator Statement",
      "scope": [
        "punctuation.terminator.statement"
      ],
      "settings": {
        "foreground": "#F50057"
      }
    },
    {
      "name": "Control flow keywords",
      "scope": "keyword.control",
      "settings": {
        "foreground": "#B388FF"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "punctuation.decorator",
        "meta.decorator variable.other.readwrite",
        "meta.decorator entity.name.function",
        "source.python entity.name.function.decorator",
        "source.python meta.function.decorator support.type"
      ],
      "settings": {
        "foreground": "#CDDC39"
      }
    },
    {
      "name": "keyword.other.unit",
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#9FA8DA"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#03A9F4"
      }
    },
		{
			"name": "Types declaration and references",
			"scope": [
				"meta.return-type",
				"support.class",
				"support.type",
				"entity.name.type",
				"entity.name.class",
				"storage.type.cs",
				"storage.type.generic.cs",
				"storage.type.modifier.cs",
				"storage.type.variable.cs",
				"storage.type.annotation.java",
				"storage.type.generic.java",
				"storage.type.java",
				"storage.type.object.array.java",
				"storage.type.primitive.array.java",
				"storage.type.primitive.java",
				"storage.type.token.java",
				"storage.type.groovy",
				"storage.type.annotation.groovy",
				"storage.type.parameters.groovy",
				"storage.type.generic.groovy",
				"storage.type.object.array.groovy",
				"storage.type.primitive.array.groovy",
				"storage.type.primitive.groovy"
			],
			"settings": {
				"foreground": "#FFD54F"
			}
		},
		{
			"name": "Types declaration and references, TS grammar specific",
			"scope": [
				"meta.type.cast.expr",
				"meta.type.new.expr",
				"support.constant.math",
				"support.constant.dom",
				"support.constant.json",
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#FFD54F"
			}
		},
    {
      "name": "Function call",
      "scope": [
        "entity.name.function",
        "support.function",
        "support.constant.handlebars",
        "source.python meta.function-call.generic"
      ],
      "settings": {
        "foreground": "#8BC34A"
      }
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#9FA8DA"
      }
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#03A9F4"
      }
    },
    {
      "name": "Html punctuations tags",
      "scope": [
        "punctuation.definition.tag",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#03A9F4b0"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#CE93D8"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {
        "foreground": "#CE93D8"
      }
    },
    {
      "name": "Library class/type",
      "scope": [
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#CE93D8"
      }
    },
    {
      "name": "Library variable",
      "scope": "support.other.variable",
      "settings": {
        "foreground": "#D4D4D4"
      }
    },
    {
      "name": "diff: deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#DC322F"
      }
    },
    {
      "name": "diff: inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#8BC34A"
      }
    },
    {
      "name": "Markup Headings",
      "scope": "markup.heading",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ffd54f"
      }
    },
    {
      "name": "Markup Links",
      "scope": "markup.underline.link",
      "settings": {
        "foreground": "#ce93d8"
      }
    },
    {
      "name": "Markup Lists",
      "scope": "markup.list",
      "settings": {
        "foreground": "#8BC34A"
      }
    },
    {
      "name": "Markup Quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#a1887f"
      }
    },
    {
      "name": "Markup Bold",
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#D4D4AA"
      }
    },
    {
      "name": "Markup Italic",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#D4D4AA"
      }
    },
    {
      "name": "Markup Italic/Bold",
      "scope": [
        "markup.italic markup.bold"
      ],
      "settings": {
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Markup Bold/Italic",
      "scope": [
        "markup.bold markup.italic"
      ],
      "settings": {
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Markup Inline",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code punctuation.definition.markdown"
      ],
      "settings": {
        "foreground": "#18FFFF"
      }
    },
    {
      "name": "Markup Fenced Code Block",
      "scope": "markup.fenced_code",
      "settings": {
        "foreground": "#90D4D4"
      }
    }
  ]
}
